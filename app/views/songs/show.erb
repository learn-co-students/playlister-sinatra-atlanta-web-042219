<% if @temp != nil %>
  <font color="blue"><h2>⚠ <%= @temp %> ⚠</h2></font>
<% end %>

<h1>"<%= @song.name %>" by <a href="/artists/<%= @song.artist.slug %>"><%= @song.artist.name %></a></h1>

<h2>Genres</h2>
<ul>
	<% @song.genres.each do |genre|%>
		<li><h3><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></h3></li>
	<% end %>
</ul><br>

<a href="/songs">Back to all songs</a>
